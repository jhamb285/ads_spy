{
  "name": "ads-intel",
  "version": "1.0.0",
  "description": "Ad intelligence normalization and analysis",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "bot": "ts-node src/bot.ts",
    "hooks": "ts-node src/hooksPipeline.ts",
    "test": "ts-node src/test.ts",
    "start": "node dist/index.js",
    "start:pm2": "pm2 start ecosystem.config.js",
    "stop:pm2": "pm2 stop ads-intel",
    "restart:pm2": "pm2 restart ads-intel",
    "logs:pm2": "pm2 logs ads-intel",
    "cron": "ts-node src/main.ts",
    "generate-tasks": "ts-node src/generateTasks.ts",
    "api": "ts-node src/api.ts",
    "admin": "ts-node admin/server.ts",
    "migrate-brands": "ts-node scripts/migrate-brands.ts",
    "migrate:multi-pages": "ts-node scripts/migrate-multi-pages.ts",
    "verify-migration": "ts-node scripts/verify-migration.ts",
    "test-api": "ts-node scripts/test-api-endpoints.ts",
    "sync-brands": "ts-node scripts/sync-brands-from-sheet-multi-page.ts",
    "sync-brands:legacy": "ts-node scripts/sync-brands-from-sheet.ts",
    "populate-sheets": "ts-node scripts/populate-sheets-from-config.ts",
    "populate-sheets-batch": "ts-node scripts/populate-sheets-batch.ts",
    "setup-db": "psql -U postgres -d creative_os -f scripts/setup-db.sql",
    "test-competitor-analysis": "ts-node scripts/test-competitor-analysis.ts",
    "test-google-ads": "ts-node scripts/test-google-ads-analysis.ts",
    "migrate:platform": "ts-node scripts/run-platform-migration.ts",
    "start-all": "concurrently \"npm run api\" \"cd frontend && npm run dev\"",
    "start-all-prod": "pm2 start ecosystem.config.js && pm2 start src/api.ts --name ads-intel-api --interpreter ts-node && cd frontend && pm2 start npm --name ads-intel-frontend -- start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.1.1",
    "@slack/bolt": "^3.17.1",
    "@slack/web-api": "^7.0.0",
    "@types/cors": "^2.8.19",
    "apify-client": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.22.1",
    "fluent-ffmpeg": "^2.1.3",
    "google-auth-library": "^10.5.0",
    "google-spreadsheet": "^5.0.2",
    "googleapis": "^133.0.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.20.0",
    "pg": "^8.11.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.11",
    "@types/pg": "^8.10.0",
    "@types/uuid": "^9.0.8",
    "concurrently": "^9.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
